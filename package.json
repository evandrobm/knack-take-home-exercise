{
  "name": "knack-coding-exercise",
  "version": "1.0.0",
  "description": "Knack Node Coding Exercise",
  "main": "src/index.ts",
  "scripts": {
    "start:dev": "nodemon",
    "test": "jest",
    "test:dev": "jest --coverage --watchAll",
    "lint": "eslint . --ext .ts",
    "build": "rimraf ./build && tsc",
    "start": "node build/src/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "keywords": [
    "knack"
  ],
  "author": "Evandro Machado",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
